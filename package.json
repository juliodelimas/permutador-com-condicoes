{
    "name": "permutador-com-condicoes",
    "version": "2.0.0",
    "description": "Ferramenta para gerar permutações de dados utilizando técnica de partição de equivalência para testes de software",
    "main": "index.html",
    "scripts": {
        "start": "live-server . --port=8000 --open",
        "server": "node server.js",
        "dev": "live-server . --port=3000 --open",
        "serve": "live-server . --port=8000",
        "format": "prettier --write \"**/*.{html,css,js,json}\""
    },
    "keywords": [
        "testing",
        "permutation",
        "equivalence-partitioning",
        "test-cases",
        "software-testing",
        "decision-table",
        "gherkin",
        "template-generator"
    ],
    "author": "Desenvolvido para a comunidade de testes",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/juliodelimas/permutador-com-condicoes.git"
    },
    "bugs": {
        "url": "https://github.com/juliodelimas/permutador-com-condicoes/issues"
    },
    "homepage": "https://github.com/juliodelimas/permutador-com-condicoes#readme",
    "dependencies": {
        "live-server": "^1.2.2"
    },
    "devDependencies": {
        "prettier": "^2.8.0"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}